{
  "compilerOptions": {
    "outDir": "./dist/",
    "sourceMap": true,
    "noImplicitAny": false,
    "module": "commonjs",
    "lib": ["dom", "es2016", "es2017", "es7", "es2020"],
    "target": "es2020",
    "allowSyntheticDefaultImports": true,
    "stripInternal": true,
    "skipLibCheck": true,
    "alwaysStrict": true,
    "forceConsistentCasingInFileNames": true,
    "noImplicitReturns": true,
    "strict": false,
    "noUnusedLocals": false,
    "jsx": "react",
    "types": []
  },
  "lib": ["esnext", "dom"],
  "exclude": [
    "node_modules"
  ]
}